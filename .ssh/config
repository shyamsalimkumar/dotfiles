#Github.com
Host github
    HostName github.com
    User git
    IdentityFile ~/.ssh/github
    IdentitiesOnly=yes

#Bitbucket.org
Host bitbucket
    HostName bitbucket.com
    User git
    IdentityFile ~/.ssh/bitbucket
    IdentitiesOnly=yes

#Openshift.com
Host openshift
    HostName rhcloud.com
    User git
    IdentityFile ~/.ssh/openshift
    IdentitiesOnly=yes

#Heroku.com
Host heroku
    HostName heroku.com
    User git
    IdentityFile ~/.ssh/heroku
    IdentitiesOnly=yes
