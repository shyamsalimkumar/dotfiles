[user]
    name = Shyam S Kumar
    email = shyam.salim.kumar@outlook.com
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
    excludesfile = /home/shyam/.gitignore
[credential]
    helper = cache
[push]
    default = matching
[alias]
	pretty-graph = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
